; lmusbdll.def : Declares the module parameters for the DLL.

LIBRARY      "lmusbdll"
; DESCRIPTION "A simple library allowing low level access to a WinUSB device"
VERSION      1.1

EXPORTS
    ; Explicit exports can go here
    InitializeDevice            @1
    TerminateDevice             @2
    WriteUSBPacket              @3
    ReadUSBPacket               @4
    InitializeDeviceByIndex     @5
    Endpoint0Transfer           @6
